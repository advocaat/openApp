<head>
    <title>Sign Up</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <%include partials/sources.ejs%>

</head>
<body>

<%include partials/header.ejs%>
<div id="mainBody" class='container'>
    <div class="col-xs-4"></div>
    <div class="col-xs-4">
        <form class='form-signup' action='/signup' method='POST'>
            <br>
            <label>Username</label><input type='text' name='username' class='form-control' placeholder='Username' required autofocus>
            <label>Password</label><input type='password' name='password' class='form-control' placeholder='Password' required>
            <br />
            <button class='btn btn-lg btn-primary btn-block' type='submit'>Register</button>
        </form>
    </div>
</div>
</body>
</html>



